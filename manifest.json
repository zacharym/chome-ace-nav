{
    "manifest_version": 2,

    "name": "Ace-Jump-Navigator",
    "description": "Follow any link on the screen easily with the keyboard",
    "version": "0.0.1",

    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "css": ["ace.css"],
            "js": ["jquery.js", "ace.js"]
        }
    ],
    "permissions": ["activeTab"]
}
